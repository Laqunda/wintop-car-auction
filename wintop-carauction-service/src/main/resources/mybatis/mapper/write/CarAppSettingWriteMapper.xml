<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wintop.ms.carauction.mapper.write.ICarAppSettingWriteDao">
  <resultMap id="BaseResultMap" type="com.wintop.ms.carauction.entity.CarAppSetting">
    <result column="id" jdbcType="BIGINT" property="id" />
    <result column="code" jdbcType="VARCHAR" property="code" />
    <result column="content" jdbcType="VARCHAR" property="content" />
    <result column="remarks" jdbcType="VARCHAR" property="remarks" />
  </resultMap>
  <sql id="Base_Column_List" >
    id,code,content,remarks
  </sql>
  <update id="updateSelective" parameterType="com.wintop.ms.carauction.entity.CarAppSetting">
    update car_app_setting
    <set>
      <if test="content != null">
        content = #{content}
      </if>
    </set>
    <where>
      <if test="code != null and code != ''">
        code = #{code}
      </if>
    </where>
  </update>

</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wintop.ms.carauction.mapper.write.ICarPhotoTempWriteDao" >
    <insert id="insertCarPhotoTemp" parameterType="com.wintop.ms.carauction.entity.CarPhotoTemp" >
        insert into car_photo_temp (id, main_photo)
        values
        <foreach collection="list" item="carPhotoTemp" index="index" separator=",">
            (
            #{carPhotoTemp.id,jdbcType=INTEGER},#{carPhotoTemp.mainPhoto,jdbcType=VARCHAR}
            )
        </foreach>
    </insert>
    <delete id="clearCarPhotoTemp">
        DELETE  FROM  car_photo_temp WHERE  id <![CDATA[ > ]]> -1
    </delete>
</mapper>
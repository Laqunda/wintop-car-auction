<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wintop.ms.carauction.mapper.write.CarAssessLogWriteDao">
    <insert id="insertCarAssessLog" parameterType="com.wintop.ms.carauction.entity.CarAssessLog">
        insert into car_assess_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null  ">id,</if>
            <if test="assessId != null  ">assess_id,</if>
            <if test="logMsg != null  and logMsg != ''  ">log_msg,</if>
            <if test="createTime != null  ">create_time,</if>
            <if test="createPerson != null  ">create_person,</if>
            <if test="userName != null  and userName != ''  ">user_name,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null  ">#{id},</if>
            <if test="assessId != null  ">#{assessId},</if>
            <if test="logMsg != null  and logMsg != ''  ">#{logMsg},</if>
            <if test="createTime != null  ">#{createTime},</if>
            <if test="createPerson != null  ">#{createPerson},</if>
            <if test="userName != null  and userName != ''  ">#{userName},</if>
        </trim>
    </insert>

    <update id="updateCarAssessLog" parameterType="com.wintop.ms.carauction.entity.CarAssessLog">
        update car_assess_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="assessId != null  ">assess_id = #{assessId},</if>
            <if test="logMsg != null  and logMsg != ''  ">log_msg = #{logMsg},</if>
            <if test="createTime != null  ">create_time = #{createTime},</if>
            <if test="createPerson != null  ">create_person = #{createPerson},</if>
            <if test="userName != null  and userName != ''  ">user_name = #{userName},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteCarAssessLogById" parameterType="Long">
        delete from car_assess_log where id = #{id}
    </delete>

    <delete id="deleteCarAssessLogByIds" parameterType="String">
        delete from car_assess_log where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>
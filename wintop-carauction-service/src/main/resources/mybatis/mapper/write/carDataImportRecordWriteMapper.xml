<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wintop.ms.carauction.mapper.write.ICarDataImportRecordWriteDao" >
    <update id="updateCarDataImportRecord" parameterType="java.lang.Long">
        update car_data_import_record set id_record=id_record+1000 where id=#{id}
    </update>

    <insert id="insertCarDataImportRecord" parameterType="com.wintop.ms.carauction.entity.CarDataImportRecord">
        insert into
         car_data_import_record
         VALUE (#{id},#{idRecord})
    </insert>
</mapper>